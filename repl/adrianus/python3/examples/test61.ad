class Flag {
    colors = ["red", "white", "yellow"]

    def getColors() {
        return colors
    }
}

class Country {
    flag = Flag()

    def constructor(name) {
        this.name = name
    }

    def getFlag() {
        return flag
    }
}

class Continent {
    america = Country("USA")

    def constructor(name) {
        this.name = name
    }

    def getCountry() {
        return america
    }
}

north_america = Continent("North America")
print (north_america.name)
print (north_america.getCountry().name)
print (north_america.getCountry().getFlag().colors)
print (north_america.getCountry().getFlag().getColors())

