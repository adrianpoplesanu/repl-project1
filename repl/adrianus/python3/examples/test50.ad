/* dp algo for longest growing subset */
a = [1, 9, 2, 9, 3, 9, 4, 9, 2, 1, 0]
n = len(a)
dp = list(11, 0)

for(i = 0; i < n; i++) {
    dp[i] = 1;
    for (j = 0; j < i; j++) {
        if (a[j] < a[i]) {
            if (dp[j] + 1 > dp[i]) {
                dp[i] = dp[j] + 1;
            }
        }
    }
}

print(dp)