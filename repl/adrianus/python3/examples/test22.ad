class Server {
    port = 8080
}

class Offseter {
    offset1 = 2

    def multiply_offset(factor) {
        return offset1 * factor
    }
}

server = Server()
offset = Offseter()

listening_port = server.port + offset.offset1
listening_port2 = server.port + offset.multiply_offset(2)

print(listening_port)
print(listening_port2)
