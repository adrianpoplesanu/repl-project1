class Dog {
    age = 0
    speak = "wooof"
    ham = "ham ham ham"

    def bark() {
        println(ham)
    }
}

class Dex : Dog {
    age = 12

    def bark() {
        println(speak)
    }

    def bark2() {
        super(Dog).bark()
    }

    def initial_age() {
        println(super(Dog).age)
    }
}

dex = Dex()
println(dex.age)
dex.bark()
dex.bark2()
dex.initial_age()

/*
asta e output-ul asteptat:
