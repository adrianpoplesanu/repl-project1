    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
            "scope": "constant.language.boolean.true.adlang",
            "settings": {
                "foreground": "#48f782",  // or any color you want
                "fontStyle": "italic"
                }
            },
            {
            "scope": "constant.language.boolean.false.adlang",
            "settings": {
                "foreground": "#f63943",  // or any color you want
                "fontStyle": "italic"
                }
            },
            {
            "scope": "entity.name.def.adlang",
            "settings": {
                "foreground": "#f65b14",  // or any color you want
                "fontStyle": "bold"
                }
            },
            {
            "scope": "entity.name.fun.adlang",
