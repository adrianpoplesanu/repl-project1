th1 = __thread("thread 1")
th2 = __thread("thread 2")
th3 = __thread("thread 3")

def runner1() {
    print ("starting thread")
    for(i1 = 0; i1 < 100; i1++) {
        print (i1)
    }
    print ("finishing thread")
}

def runner2() {
    print ("starting thread")
    for(i2 = 0; i2 < 100; i2++) {
        print (i2)
    }
    print ("finishing thread")
}

def runner3() {
    print ("starting thread")
    for(i3 = 0; i3 < 100; i3++) {
        print (i3)
    }
    print ("finishing thread")
}


th1.callback(runner1)
th2.callback(runner2)
th3.callback(runner3)

th1.runAsync()
th2.runAsync()
th3.runAsync()