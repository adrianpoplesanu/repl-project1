class Flag {
    colors = ["red", "white", "purple"]
}

class Country {
    flag = Flag()

    def constructor(name) {
        this.name = name
    }

    def getFlag() {
        return flag;
    }
}

class Continent {
    country = Country("USA")
}

america = Continent()
print (america.country.getFlag().colors[1])

america.country.getFlag().colors = ["brown", "purple", "cyan"]

print (america.country.getFlag().colors[1])

america.country.getFlag().colors[1] = "yellow"

print (america.country.getFlag().colors)