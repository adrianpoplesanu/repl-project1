sol = [0, 0, 0]

def check_sol(k) {
    i = 0
    while ( i < k ) {
        if (sol[i] == sol[k]) { return false; }
        i = i + 1
    }
    return true;
}

def back(k) {
    if (k == 3) {
        print(sol)
    } else {
        v = 0
        while(v < 5) {
            sol[k] = v
            if (check_sol(k)) {
                back(k + 1)
            }
            v = v + 1
        }
        sol[k] = 0
    }
}

back(0)
