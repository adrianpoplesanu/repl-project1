class User {
    name = null
    firstname = null
    age = null

    def init(p_name, p_firstname, p_age) {
        name = p_name
        firstname = p_firstname
        age = p_age
    }

    def display() {
        print(name)
        print(firstname)
        print(age)
    }
}

class Service {
    def createUser() {
        user = User()
        user.init("bebe", "dex", 11)
        return user
    }
}

service = Service()
user = service.createUser()
user.display()
