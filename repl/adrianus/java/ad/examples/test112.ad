th1 = __thread("thread 1")
th2 = __thread("thread 2")
th3 = __thread("thread 3")

class Task {
    def runner() {
        for(i = 0; i < 100; i++) {
            println(i)
        }
    }
}

task1 = Task()
th1.execute(task1.runner)

task2 = Task()
th2.execute(task2.runner)

task3 = Task()
th3.execute(task3.runner)

th1.start()
th2.start()
th3.start()

println("after threads")
